{
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "terminal.integrated.lineHeight": 1.2,
  "editor.fontFamily": "FantasqueSansM Nerd Font",
  "editor.fontSize": 14,
  "editor.lineHeight": 1.4,
  "explorer.confirmDelete": false,
  "workbench.trustedDomains.promptInTrustedWorkspace": false,
  "window.menuBarVisibility": "toggle",
  "debug.console.fontSize": 13,
  "editor.renderWhitespace": "boundary",
  "files.insertFinalNewline": true,
  "python.venvFolders": [
    "~/.local/share/pypoetry/virtualenvs",
  ],
  "editor.minimap.enabled": false,
  "security.workspace.trust.enabled": false,
  "[makefile]": {
    "editor.detectIndentation": true,
  },
  "workbench.iconTheme": "dot_colored_gray",
  "workbench.tree.indent": 14,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "excalidraw.theme": "auto",
  "[python]": {
    "editor.formatOnType": true,
  },
  "hediet.vscode-drawio.resizeImages": null,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker",
  },
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml",
  },
  "ruff.showNotifications": "onWarning",
  "git.openRepositoryInParentFolders": "never",
  "hediet.vscode-drawio.theme": "min",
  "window.titleBarStyle": "native",
  "window.autoDetectColorScheme": true,
  "window.title": "${rootPath}${separator}Code",
  "breadcrumbs.enabled": false,
  "editor.renderLineHighlight": "none",
  "terminal.external.linuxExec": "tilix",
  "redhat.telemetry.enabled": false,
}
